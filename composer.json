{
    "name": "cakephp/upgrade",
    "description": "Command line tool for updating CakePHP applications and plugins.",
    "homepage": "http://cakephp.org",
    "type": "project",
    "license": "MIT",
    "require": {
        "cakephp/console": "^4.0",
        "rector/rector": "~0.6"
    },
    "autoload": {
        "psr-4": {
            "Cake\\Upgrade\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Upgrade\\Test\\": "tests",
            "OldApp\\": "tests/OldApp"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable": true,
    "support": {
        "source": "https://github.com/cakephp/upgrade"
    },
    "scripts": {
        "cs-check": "phpcs --colors --parallel=16 -p -s src/ tests/",
        "cs-fix": "phpcbf --colors --parallel=16 -p src/ tests/",
        "test": "phpunit",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "require-dev": {
        "cakephp/cakephp": "^4.0",
        "cakephp/cakephp-codesniffer": "^4.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^8.4"
    }
}
